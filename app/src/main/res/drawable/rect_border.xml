<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <inset android:insetBottom="-1dp">
            <shape>
                <stroke
                    android:width="1dp"
                    android:color="#44000000" />
            </shape>
        </inset>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <solid android:color="#fff" />
        </shape>
    </item>
</layer-list>